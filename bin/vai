depth=15
for index in {1..30}
do
    echo npm run localsearch -- --name=$instance --depth=$depth --index=$index --csv
    npm run localsearch -- --name=$instance --depth=$depth --index=$index --csv >> ~/Desktop/resultados_1_$depth_$instance.csv
done