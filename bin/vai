#!/bin/bash

for instance in abba order four "aman-aplan" triples ranges words
do
    for depth in {5..15} # depth
    do
        for index in {1..30}
        do
            echo npm run localsearch -- --name=$instance --depth=$depth --index=$index --csv
            npm run localsearch -- --name=$instance --depth=$depth --index=$index --csv >> ~/Desktop/resultados_ILS.csv
        done
    done
done